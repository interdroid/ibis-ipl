JAVACLASSPATH=$IBIS_ROOT/ibis/classlibs:$IBIS_ROOT:.:$JAVA_ROOT/jre/lib/rt.jar:$JAVA_ROOT/jre/lib/core.jar:$JAVA_ROOT/jre/lib/server.jar:$JAVA_ROOT/jre/lib/core.jar:$JAVA_ROOT/jre/lib/server.jar:$JAVA_ROOT/jre/lib/security.jar

export LD_ASSUME_KERNEL=2.2.5

export LFC_INTR_FIRST=100
export IBP_SEND_SYNC=100 
export MANTA_NO_PANDA=1
export PAN_COMM_NO_IDLE_POLL=1

Dlibpath="-Dsun.boot.library.path=$IBIS_ROOT/ibis/lib:$JAVA_ROOT/jre/bin:$JAVA_ROOT/jre/lib/i386"

Dpool_host_num="-Dpool_host_number=$1"
shift

Dprops="-Dibis.property.file=$IBIS_ROOT/ibis/properties"

Dpool_total="-Dpool_total_hosts=$1"
shift

if [ -z "$PRUN_ENV" ]
then
    Dns_pool="-Dname_server_pool=$1"
    shift
else
    Dns_pool="-Dname_server_pool=$PRUN_ENV"
fi

Dns_port="-Dname_server_port=$1"
shift

Dns_server="-Dname_server=$1"
shift

$JAVA_ROOT/bin/java $Dlibpath $Dpool_host_num $Dprops  $Dpool_total $Dns_pool $Dns_port $Dns_server -Dhosts="$HOSTS" -Xmx400M -Xbootclasspath:$JAVACLASSPATH -classpath $JAVACLASSPATH "$@"
