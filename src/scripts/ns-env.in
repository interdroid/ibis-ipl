#
# This file contains definitions for the ibis nameserver.
# It can be modified by the user and survives reconfiguration of the
# Ibis tree (but not distclean).
#
export IBIS_NAMESERVER_HOST="@IBIS_NS_HOST@"
export IBIS_NAMESERVER_PORT="@IBIS_NS_PORT@"

# retry connection to nameserver if it is not there? Set to "yes" or "no".
export IBIS_NAMESERVER_RETRY="no"

# not nameserver related, but re-configuration-proof:
# set to 1 if javawrapper must be used
export USE_JAVAWRAPPER=0
